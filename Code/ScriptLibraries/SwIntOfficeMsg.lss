Dim SW_ExistingAttachmentReadMode As String
Dim SW_Convert2PDFOption As Integer
Dim SW_ShowContentLibMenus As Variant
Dim SW_VisibleVBAMacroMenuItems As Long
Dim SW_EnabledVBAMacroMenuItems As Long

Dim SW_SuperUser_Domain As String
Dim SW_SuperUser_UserName As String
Dim SW_SuperUser_Password As String

Dim SW_DetachedFileProtectionType As String
Dim SW_DoCloseOfficeApp As Variant
Dim SW_DisableWordAddins As String
'//***********************************************************************************

'++LotusScript Development Environment:2:2:Initialize:1:10
Sub Initialize
	' global variables 
	SW_ExportDataSeparator = "|"
'Change the value of the following four variables only if you are setting up a separate templates database	
	SW_TemplateDbServer = ""		'Use in documents database to point to templates database server
	SW_TemplateDbFilePath = ""	    	'Use in documents database to point to templates database, relative to data directory (e.g. "IntOffice\MyTemplates.nsf")
	SW_DocumentsDbServer = ""		'Use in templates database to point to documents database server
	SW_DocumentsDbFilePath = ""	 'Use in templates database to point to documents database, relative to data directory (e.g. "IntOffice\MyDocuments.nsf")
	SW_TemplatePassword = ""
	SW_EnableOfficeMacros = True	' set this value to False, if you want not to use macros in Office
	SW_AttVersion = ""					' string; if you are using version control, assign version value to this global variable
	SW_DettachInDialog = True		' if True "Detach" option is displayed in dialog (read mode).
	SW_ShowStatusDialog = False
	SW_Prompt2SaveDoc = 1
	SW_VBATemplatesPrefix = ""
	SW_MissingTemplateBehaviour = 1
	SW_ExistingAttachmentReadMode = "1"		' "1"=Display dialog box; "2" = Open in READ mode; "3" = Open in EDIT mode
	SW_Convert2PDFOption = 0		' 	0 = Prompt user; 1 = Save to disk; 2 = Send by e-mail; 3 = Save as new attachment; 4 = Replace existing attachment
	SW_ShowContentLibMenus = True	' set this value to False, if you don't want to use content library features from within MS Word
	
	SW_SuperUser_Domain = ""
	SW_SuperUser_UserName = ""
	SW_SuperUser_Password = ""
	
	'NEW IN VERSION 5
	SW_DetachedFileProtectionType = "READ_ONLY" ' "PASSWORD" = Protect doc with internal password | "READ_ONLY" = Protect doc by opening it in read only mode
	SW_DoCloseOfficeApp = False 'Determines whether to close Office application after use, if it has 0 opened documents
	SW_DisableWordAddins = "rhd.dot"
	
'	Variable SW_VisibleVBAMacroMenuItems determines which entries in Lotus Notes submenus in MS Office applications will be visible.
'	It is a sum of the following symbolic constants:
'	SW_VBA_MENU_SAVE_TO_NOTES -> Save to Notes
'	SW_VBA_MENU_SAVE_CLOSE_OFFICE -> Save and close Office document
'	SW_VBA_MENU_SAVE_CLOSE_NOTES -> Save and close Notes document	
'	SW_VBA_MENU_INSERT_FROM_CONTENT_LIB -> Insert from content library
'	SW_VBA_MENU_SAVE_TO_CONTENT_LIB -> Save to content library
'	SW_VBA_MENU_CONVERT_SEND_PDF -> Convert to PDF and E-mail
'	SW_VBA_MENU_SAVE_PDF -> Save as PDF
'	
%REM	
	SW_VisibleVBAMacroMenuItems = _
	SW_VBA_MENU_SAVE_TO_NOTES + _
	SW_VBA_MENU_SAVE_CLOSE_OFFICE + _
	SW_VBA_MENU_SAVE_CLOSE_NOTES + _
	SW_VBA_MENU_INSERT_FROM_CONTENT_LIB + _
	SW_VBA_MENU_SAVE_TO_CONTENT_LIB + _
	SW_VBA_MENU_CONVERT_SEND_PDF + _
	SW_VBA_MENU_SAVE_PDF
%END REM
	SW_VisibleVBAMacroMenuItems = _
	SW_VBA_MENU_SAVE_TO_NOTES + _
	SW_VBA_MENU_SAVE_CLOSE_OFFICE + _
	SW_VBA_MENU_SAVE_CLOSE_NOTES + _
	SW_VBA_MENU_INSERT_FROM_CONTENT_LIB + _
	SW_VBA_MENU_SAVE_TO_CONTENT_LIB + _
	SW_VBA_MENU_CONVERT_SEND_PDF + _
	SW_VBA_MENU_SAVE_PDF
	
	SW_EnabledVBAMacroMenuItems = _
	SW_VBA_MENU_SAVE_TO_NOTES + _
	SW_VBA_MENU_SAVE_CLOSE_OFFICE + _
	SW_VBA_MENU_SAVE_CLOSE_NOTES + _
	SW_VBA_MENU_INSERT_FROM_CONTENT_LIB + _
	SW_VBA_MENU_SAVE_TO_CONTENT_LIB + _
	SW_VBA_MENU_CONVERT_SEND_PDF + _
	SW_VBA_MENU_SAVE_PDF
'//*********************************
End Sub

'++LotusScript Development Environment:2:1:SW_IntOfficeMsg:1:8
Function SW_IntOfficeMsg (MsgNo As Integer) As String
%REM
Defined in
	SWMessageLib
Parameters
	MsgNo
		Number of message text.
Return Value
	String
		Message text.
%END REM
	Execute(|SW_tmpIntOfficeMsg$ = SW_IntOfficeMsg| + Cstr(MsgNo))
	SW_IntOfficeMsg = SW_tmpIntOfficeMsg$
End Function