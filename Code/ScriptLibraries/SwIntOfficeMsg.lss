'//*************************Constants for VBA menu options******************
'//*************************DO NOT MODIFY THEIR VALUES****************
Const SW_VBA_MENU_SAVE_TO_NOTES& = 1
Const SW_VBA_MENU_SAVE_CLOSE_OFFICE& = 2
Const SW_VBA_MENU_SAVE_CLOSE_NOTES& = 4
Const SW_VBA_MENU_INSERT_FROM_CONTENT_LIB& =8
Const SW_VBA_MENU_SAVE_TO_CONTENT_LIB& = 16
Const SW_VBA_MENU_CONVERT_SEND_PDF& = 32
Const SW_VBA_MENU_SAVE_PDF& = 64



'//************************* Global variable declaration ************************
