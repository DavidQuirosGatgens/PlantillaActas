		return ""
	}
}
var urldb:String=context.getUrl().toString();
var nIndexOf = urldb.indexOf(".nsf");
nIndexOf=nIndexOf+4;
urldb = urldb.substring(0, nIndexOf);

return url + "/(Parametros)/Parametros/$File/"+imagen+"?OpenElement"}]]></xp:this.value></xp:text>
