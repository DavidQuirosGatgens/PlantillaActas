			<xp:td><xp:button id="NewDocButton" style="margin-top:15px;margin-left:25px;font-family:Segoe UI,sans-serif;font-weight:bold;font-size:8pt;height:30px;vertical-align:middle">
			<xp:this.rendered><![CDATA[#{javascript:var db:NotesDatabase=session.getCurrentDatabase();
var doc:NotesDocument=currentDocument.getDocument(true);
var valString:String = "";


if (context.getUser().getRoles().contains('[Secretaria]')) {

	if(!currentDocument.isNewNote()){

		if (compositeData.dataSource.isEditable()) {
		
			if (doc.hasItem('SWAttachment')){ // aplica para el docTema cuando se crea desde la opcion Temas Pendientes
				var nrt:NotesRichTextItem=doc.getFirstItem('SWAttachment');
				if (nrt!=null){
    				var eos:java.util.Vector = nrt.getEmbeddedObjects();
      				if (eos.isEmpty()) {
        				valString = 'Nothing found';
      				} else {
          				var v;
     	   				for (v = 0;v < eos.size();v++){  // revisa si el documento tiene un archivo adjunto
        					var eo:NotesEmbeddedObject = eos.get(v);
        					valString = valString + eo.getName() + ' - ' ;
    					}
					}
				}	
				if (valString=='Nothing found') {return true;}
				return false;
			}  // fin SWAttachment
	
			// para el formulario de la Orden del dÃ­a & Tema
			// ----------------------------------------------------	
			var field = document1.getValue( 'Form' );
			if (field=='Acta' |field=='Tema'){
				if (doc.hasItem('SWAttachment')){
					var nrt:NotesRichTextItem=doc.getFirstItem('SWAttachment');
					if (nrt!=null){
    					var eos:java.util.Vector = nrt.getEmbeddedObjects();
      					if (eos.isEmpty()) {
        					valString = 'Nothing found';
      					} else {
          					var v;
     	   					for (v = 0;v < eos.size();v++){
	        					var eo:NotesEmbeddedObject = eos.get(v);
    	    					valString = valString + eo.getName() + ' - ' ;}
						}
					}				
					if (valString=='Nothing found') {return true;}
						return false;
					}
					else {
						return true;
				} // fin SWAttachment
			} // fin documento Acta/Tema	
			return false;
		} // verifica que no se nuevo el documento	
	} // verifica si es editable
} // verifica el rol Secretaria

return false;
}]]></xp:this.rendered>
